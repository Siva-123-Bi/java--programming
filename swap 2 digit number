import java.util.Scanner;

public class Solution {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        String s = sc.next();
        if (s.length() < 2) {
            System.out.println("STDOUT");
            return;
        }
        char s1 = s.charAt(0);
        char s2= s.charAt(1);
       System.out.println("" + s2 + s1);
    }
}
